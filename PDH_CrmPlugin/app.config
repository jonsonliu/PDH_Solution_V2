<?xml version="1.0"?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="ProductQuantitySoapSOAP" />
                <binding name="ProductStatusSoapSOAP" />
                <binding name="ProductSoapSOAP" />
                <binding name="ProductSoapSOAP1" />
                <binding name="ProductSoapSOAP2" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://petdreamhouse.co.uk/soap/product_quantity_soap.php"
                binding="basicHttpBinding" bindingConfiguration="ProductQuantitySoapSOAP"
                contract="ProductQuantityService.ProductQuantitySoap" name="ProductQuantitySoapSOAP" />
            <endpoint address="http://petdreamhouse.co.uk/soap/product_status_soap.php"
                binding="basicHttpBinding" bindingConfiguration="ProductStatusSoapSOAP"
                contract="ProductStatusService.ProductStatusSoap" name="ProductStatusSoapSOAP" />
            <endpoint address="http://petdreamhouse.co.uk/soap/product_delete_soap.php"
                binding="basicHttpBinding" bindingConfiguration="ProductSoapSOAP2"
                contract="ProductDeleteService.ProductSoap" name="ProductSoapSOAP2" />
            <endpoint address="http://petdreamhouse.co.uk/soap/product_soap.php"
                binding="basicHttpBinding" bindingConfiguration="ProductSoapSOAP"
                contract="ProductService.ProductSoap" name="ProductSoapSOAP" />
            <endpoint address="http://petdreamhouse.co.uk/soap/product_update_soap.php"
                binding="basicHttpBinding" bindingConfiguration="ProductSoapSOAP1"
                contract="ProductUpdateService.ProductSoap" name="ProductSoapSOAP1" />
        </client>
    </system.serviceModel>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
